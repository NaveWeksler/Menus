generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type MenusItems {
  v        Int    @map("__v")
  id       String @map("_id") @db.ObjectId
  imageURL String
  name     String
  price    String
}

model menuitems {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  v        Int    @map("__v")
  imageURL String
  name     String
  price    String
}

model menus {
  id    String       @id @default(auto()) @map("_id") @db.ObjectId
  v     Int          @map("__v")
  items MenusItems[]
  title String
}
